<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserDao">
	<insert id="insertUser" parameterType="pojo.UserBean" >
		insert into user (username,password,email,telephone) values(#{username},#{password},#{email},#{telephone})
	</insert>
	<select id="queryUserOne" parameterType="String" resultType="pojo.UserBean">
		select * from user where username=#{username}
	</select>


</mapper>

